{
  "version": 2,
  "headers": [
    {
      "source": "/",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://app.repspark.com" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors https://app.repspark.com" }
      ]
    }
  ]
}
